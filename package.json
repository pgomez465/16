{
  "name": "lemon",
  "version": "0.1.0",
  "description": "A real time video and text based chat client using P2P connections.",
  "main": "lib/index.js",
  "scripts": {
    "start": "node lib/index.js",
    "start:watch": "nodemon -w lib lib/index.js",
    "build": "babel src --out-dir lib",
    "build:watch": "babel --watch src --out-dir lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack --progress",
    "webpack:watch": "webpack --progress --watch",
    "lint": "eslint src",
    "less": "lessc client/less/styles.less static/css/styles.css",
    "less:watch": "nodemon -w client/less -e less,css --exec \"lessc client/less/styles.less static/css/styles.css\""
  },
  "author": "nulifier",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.14.2",
    "bookshelf": "^0.9.1",
    "bootstrap": "^3.3.6",
    "credential": "^1.0.0",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "express-session": "^1.12.1",
    "handlebars": "^4.0.5",
    "knex": "^0.9.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "socket.io": "^1.3.7"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-flow-strip-types": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-node5": "^1.1.1",
    "browser-sync": "^2.10.1",
    "eslint": "^6.8.0",
    "jquery": "^2.1.4",
    "less": "^2.5.3",
    "nodemon": "^1.8.1",
    "socket.io-client": "^1.3.7",
    "sqlite3": "^3.1.1",
    "webpack": "^1.12.9"
  },
  "babel": {
    "plugins": [
      "transform-flow-strip-types"
    ],
    "presets": [
      "es2015-node5"
    ]
  }
}
